{
   "extends": ["airbnb-base", "airbnb-typescript/base", "plugin:security/recommended"],
   "parserOptions": {
     "sourceType" : "module",
     "project": "./tsconfig.json"
   },
   "ignorePatterns": [
      "node_modules/*"
    ],
   "rules": {
    "@typescript-eslint/indent": ["error", 4],
    "max-len": "off",
    "require-jsdoc": "off",
    "eqeqeq": ["error", "always"],
    "quotes": ["error", "single"],
    "linebreak-style": 0
    }
  }